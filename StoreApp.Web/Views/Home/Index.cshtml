@model ProductViewListModel
<p>Index Page</p>

@if(Model.products.Count()==0)
{
    <p>Ürün bulunamadı</p>
}

@foreach(var p in Model.products)
{
    <div>
        <h4>@p.Name</h4>
        <p>@p.Description</p>
        <p>@p.Price</p>
    </div>
}

@*@for(var i = 1 ; i <= Model.PageInfo.TotalPage; i++){

    <a href="/?Page=@i">@i</a>
    

}*@
<div page-model="@Model.PageInfo" page-action="Index"></div>